                <Button Style="{StaticResource SomeButtonStyle}" x:Name="btnImage24" Click="btnImage24_Click" PreviewMouseDown="Button_PreviewMouseDown" Height="150" Width="150" Margin="700,-362,-280,10">

                    <Image Name="image24" Source="imagini/animal12.png" Uid="12"/>

                </Button>
            </Grid>

            <Button Style="{StaticResource SomeButtonStyle}" x:Name="btnNextLevel" Click="btnNextLevel_Click" Height="40" Width="40" Margin="1002,314,56,374" BorderThickness="0" RenderTransformOrigin="-2.031,0.5">

                <Button.Background>
                    <ImageBrush ImageSource="imagini/nextLevel.png"/>
                </Button.Background>


            </Button>

            <Button Style="{StaticResource SomeButtonStyle}" x:Name="btnHelp" Click="btnHelp_Click" Height="40" Width="40" Margin="1002,616,0,72" BorderThickness="0" RenderTransformOrigin="-2.031,0.5" HorizontalAlignment="Left">

                <Button.Background>
                    <ImageBrush ImageSource="imagini/help.png"/>
                </Button.Background>
            </Button>

            <Button Style="{StaticResource SomeButtonStyle}" Click="btnBackToLogin_Click" x:Name="btnBackToLogin" Height="40" Width="40" Margin="1002,468,56,220" BorderThickness="0" RenderTransformOrigin="-2.031,0.5">

                <Button.Background>
                    <ImageBrush ImageSource="imagini/back.png"/>
                </Button.Background>
            </Button>

            <TextBlock x:Name="gameCountDownPlay" Margin="980,152,14,555" TextWrapping="Wrap" FontFamily="Segoe UI Semibold" RenderTransformOrigin="-0.217,0.373" Foreground="#FF71D1A4" FontWeight="Bold" Text="Secunde ramase:">
                <TextBlock.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform Angle="0.015"/>
                        <TranslateTransform/>
                    </TransformGroup>
                </TextBlock.RenderTransform>
            </TextBlock>
            <TextBlock x:Name="gameCountDownBeforeStart" Margin="0,165,50,0" TextWrapping="Wrap" FontSize="28" FontFamily="Segoe UI Semibold" RenderTransformOrigin="-0.217,0.373" Width="42" Foreground="#FF71D1A4" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Right" Height="38"><Run Text="75"/><LineBreak/><Run/></TextBlock>

        </Grid>

    </Border>
</Window>
